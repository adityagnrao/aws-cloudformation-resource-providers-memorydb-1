{
  "CreateInputs": {
    "ClusterName": "cfncontractcluster",
    "Description": "memdb cfn test cluster",
    "NodeType": "db.r6g.large",
    "NumShards": 1,
    "NumReplicasPerShard": 1,
    "ACLName": "open-access",
    "EngineVersion": "6.2",
    "TLSEnabled": true,
    "Port": 6379,
    "ParameterGroupName": "default.memorydb-redis6",
    "AutoMinorVersionUpgrade": true,
    "SubnetGroupName": "default",
    "SnsTopicArn": "{{awsmemorydbclustercto1}}",
    "KmsKeyId": "{{awsmemorydbclustercto2}}",
    "Tags": [
      {
        "Key": "service",
        "Value": "memorydb"
      }
    ]
  },
  "PatchInputs": [
    {
      "op": "replace",
      "path": "/Tags/0/Value",
      "value": "test-value"
    },
    {
      "op": "replace",
      "path": "/Tags/0/Key",
      "value": "test-key"
    },
    {
      "op": "replace",
      "path": "/Description",
      "value": "memdb cfn test cluster updated"
    },
    {
      "op": "add",
      "path": "/MaintenanceWindow",
      "value": "sat:04:30-sat:05:30"
    },
    {
      "op": "add",
      "path": "/SnapshotWindow",
      "value": "09:30-11:30"
    },
    {
      "op": "add",
      "path": "/SnapshotRetentionLimit",
      "value": 2
    }
  ]
}